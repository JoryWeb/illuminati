<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="poi_report_kardex_lot_tree" model="ir.ui.view">
			<field name="name">poi.report.kardex.lot.tree</field>
			<field name="model">poi.report.kardex.lot</field>
			<field name="arch" type="xml">
				<tree string="Kardex de Productos">
					<field name="date"/>
                    <field name="product_id"/>
                    <field name="lot_id"/>
                    <field name="picking"/>
                    <field name="origin"/>
                    <field name="name_mov"/>
                    <field name="origen"/>
                    <field name="destino"/>
                    <field name="cantidad"/>
                    <field name="precio"/>
                    <field name="cantidad_en_fecha"/>
                    <field name="valor_inventario_fecha"/>
				</tree>
			</field>
		</record>
		
	</data>
</odoo>